{
  "name": "boomroach-ai-memecoin",
  "version": "1.0.0",
  "description": "The ultimate AI trading platform for Solana - BoomRoach Wales 2025 Edition",
  "private": true,
  "scripts": {
    "dev": "bun run dev:all",
    "dev:frontend": "next dev -H 0.0.0.0",
    "dev:backend": "cd backend && bun run dev",
    "dev:hydra-bot": "cd hydra-bot && python3 trading_engine/main.py & python3 ai_signal_engine/main.py & python3 telegram_bot/main.py",
    "dev:all": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\" \"bun run dev:hydra-bot\"",
    "build": "next build",
    "build:backend": "cd backend && bun run build",
    "start": "bun run start:all",
    "start:frontend": "next start -H 0.0.0.0",
    "start:backend": "cd backend && bun run start",
    "start:hydra-bot": "cd hydra-bot && python3 trading_engine/main.py & python3 ai_signal_engine/main.py & python3 telegram_bot/main.py",
    "start:all": "concurrently \"bun run start:frontend\" \"bun run start:backend\" \"bun run start:hydra-bot\"",
    "lint": "biome lint --write .",
    "lint:check": "biome check .",
    "test": "bun test",
    "test:watch": "bun --watch",
    "test:coverage": "bun --coverage",
    "type-check": "tsc --noEmit",
    "hydra:engines": "cd hydra-bot && python3 -c \"from src.engines import *; print('All engines loaded successfully')\"",
    "hydra:test": "cd hydra-bot && python3 -m pytest tests/",
    "hydra:status": "curl -s http://localhost:3001/api/hydra-bot/health | jq .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next dist",
    "analyze": "cross-env ANALYZE=true next build"
  },
