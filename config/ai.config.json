{
  "defaultModel": "gpt-4",
  "supportedModels": [
    "gpt-4",
    "gpt-3.5-turbo",
    "custom-lstm",
    "custom-transformer",
    "quantum-arbitrage",
    "neural-trend-rider",
    "grid-trading",
    "momentum-scalper",
    "mean-reversion",
    "sentiment-analyzer",
    "dca-bot",
    "hybrid-ai"
  ],
  "engines": [
    "QuantumArbitrageEngine",
    "NeuralTrendRiderEngine",
    "GridTradingEngine",
    "MomentumScalperEngine",
    "MeanReversionEngine",
    "SentimentAnalyzerEngine",
    "DCABotEngine",
    "HybridAIEngine"
  ],
  "explainability": ["SHAP", "LIME", "dashboard", "feature-importance"],
  "reinforcementLearning": {
    "enabled": true,
    "algorithms": ["Q-learning", "DQN", "PPO", "A3C"],
    "rewardStrategies": ["profit", "risk-adjusted", "drawdown", "custom"]
  },
  "ensembleMethods": ["voting", "bayesian", "stacking", "blending"],
  "hyperparameterTuning": {
    "enabled": true,
    "methods": ["bayesian", "grid", "random", "multi-objective", "genetic"],
    "maxTrials": 100,
    "earlyStopping": true
  },
  "deepLearningEnhancements": [
    "multi-timeframe-transformers",
    "cross-market",
    "anomaly-detection",
    "predictive-maintenance",
    "NLP",
    "computer-vision"
  ],
  "modelVersioning": {
    "enabled": true,
    "registry": true,
    "rollback": true,
    "comparison": true
  },
  "abTesting": {
    "enabled": true,
    "strategies": ["split", "multi-arm-bandit"]
  },
  "riskManagement": {
    "maxDrawdown": 0.2,
    "maxPositionSize": 0.1,
    "stopLoss": 0.05,
    "takeProfit": 0.1
  },
  "logging": {
    "level": "info",
    "metrics": ["accuracy", "sharpe", "sortino", "maxDrawdown", "winRate"]
  }
}
